        <activity
            android:name="kr.or.camticharness.CarGameStartActivity"
            android:screenOrientation="landscape"
            android:theme="@android:style/Theme.Holo.NoActionBar" />
        <activity
            android:name="kr.or.camticharness.PenguinGameStartActivity"
            android:screenOrientation="landscape"
            android:theme="@android:style/Theme.Holo.NoActionBar" />

        <provider
            android:name="com.android.tools.fd.runtime.InstantRunContentProvider"
            android:authorities="kr.or.camticharness.com.android.tools.fd.runtime.InstantRunContentProvider"
            android:multiprocess="true" />
